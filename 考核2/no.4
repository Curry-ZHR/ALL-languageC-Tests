#include<stdio.h>
#include<ctype.h>

int put_stringr(const char s[])
{
    int i=0;
    int cnt=0;
    while (s[i]){
        cnt++;
        i++;
    }
    return cnt;
}
int main(void)
{
    char str_1[100];
    char str_2[100];
    printf("haystack=");scanf("%s",&str_1);
    printf("needle=");scanf("%s",&str_2);

    for (i=0;i<put_stringr(str_1);i++){
        while (str_1[i] == str_2[0])
        {
            printf("%d",i+1);
        }
        while (str_1[i] != str_2[0])
        {
            printf("-1");
        }

    }
    return 0;
}
